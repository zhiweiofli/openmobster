<?xml version="1.0" encoding="UTF-8"?>

<deployment xmlns="urn:jboss:bean-deployer:2.0">
   <!--
   		Registers the TwitterService "MobileServiceBean" with the JBoss Microcontainer based kernel 
    -->
   <bean name="/twitter/tweets" class="org.openmobster.hello.mobster.rpc.TwitterService">
   		<depends>services://MobileObjectMonitor</depends>
   		<depends>services://MobileServiceMonitor</depends>
   </bean>
</deployment>